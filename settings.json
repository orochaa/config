{
  "window.commandCenter": false,
  "window.menuBarVisibility": "toggle",

  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.lineHeight": 26,
  "editor.rulers": [80, 120],
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.semanticHighlighting.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.indentation": true,
  "editor.guides.highlightActiveIndentation": "always",
  "editor.guides.bracketPairs": "active",
  "editor.scrollbar.vertical": "auto",
  "editor.renderLineHighlight": "gutter",
  "editor.minimap.enabled": false,
  "editor.minimap.size": "fit",
  "editor.suggestSelection": "first",
  "editor.linkedEditing": true,
  "editor.unicodeHighlight.allowedLocales": {
    "pt-BR": true
  },

  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Omni",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.activityBar.location": "hidden",
  "workbench.statusBar.visible": true,
  "workbench.layoutControl.enabled": false,
  "workbench.list.defaultFindMode": "filter",
  "workbench.list.typeNavigationMode": "automatic",
  "workbench.list.defaultFindMatchType": "fuzzy",

  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.symbolPath": "last",
  "breadcrumbs.icons": true,

  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock*, yarn*, pnpm*, .npm*, eslint*, .eslint*, prettier*, .prettier*, tsconfig*, .nvm*",
    "vite.config.*": "tailwind*, postcss*",
    ".gitignore": ".git*",
    ".env": ".env*",
    "jest.*": ".swc*",
    "app.*": "babel*, eas*",
    "*ockerfile": "docker*, .docker*",
    "*.go": "${capture}_*.go",
    "*.ts": "${capture}.*.ts",
    "*.tsx": "${capture}.*.tsx",
    "eng.traineddata": "*.traineddata",
    "go.mod": "go.sum"
  },
  "explorer.sortOrder": "foldersNestsFiles",

  "terminal.integrated.fontSize": 15,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.env.windows": {},
  "terminal.integrated.env.linux": {},

  "git.openRepositoryInParentFolders": "always",

  "gitlens.hovers.currentLine.over": "line",
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.currentLine.scrollable": false,
  "gitlens.hovers.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,

  "zenMode.hideLineNumbers": false,
  "zenMode.centerLayout": false,
  "zenMode.hideActivityBar": true,
  "zenMode.hideStatusBar": true,
  "zenMode.showTabs": "single",

  "diffEditor.ignoreTrimWhitespace": false,

  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "never",
    "source.sortMembers": "never"
  },

  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 300,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.vscode": false,
    "**/node_modules": false
  },
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json",
    ".env*": "dotenv",
    "*.code-snippets": "json",
    "*.tsx": "typescriptreact",
    "*.css": "tailwindcss"
  },

  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma-insider"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },

  "javascript.format.enable": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.inlayHints.parameterNames.enabled": "none",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "javascript.format.semicolons": "remove",
  "javascript.preferences.importModuleSpecifier": "shortest",
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "javascript.preferences.jsxAttributeCompletionStyle": "braces",

  "typescript.format.enable": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterNames.enabled": "none",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "typescript.format.semicolons": "remove",
  "typescript.preferences.importModuleSpecifier": "shortest",
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.jsxAttributeCompletionStyle": "braces",

  "prettier.requireConfig": false,
  "prettier.bracketSpacing": true,
  "prettier.bracketSameLine": false,
  "prettier.arrowParens": "avoid",
  "prettier.endOfLine": "lf",
  "prettier.semi": false,
  "prettier.useTabs": false,
  "prettier.singleQuote": true,
  "prettier.quoteProps": "as-needed",
  "prettier.printWidth": 80,
  "prettier.trailingComma": "all",
  "prettier.jsxSingleQuote": false,
  "prettier.singleAttributePerLine": false,

  "biome.enabled": true,

  "emmet.syntaxProfiles": { "javascript": "jsx" },
  "emmet.includeLanguages": { "javascript": "javascriptreact" },

  "go.toolsManagement.autoUpdate": true,
  "go.testFlags": ["-count=1"],

  "colorize.languages": [
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg",
    "react",
    "typescriptreact"
  ],

  "tailwindCSS.experimental.classRegex": [
    ["(?:cn,clsx,twMerge)\\(([^)]*)\\)", "(?:'|\"|`)([^'\"`]*)(?:'|\"|`)"],
    ["tv\\(([^)]*)\\)", "(?:'|\"|`)([^'\"`]*)(?:'|\"|`)"],
    ["(?:class|className)\\s*:\\s*[\"'`]([^\"'`]*)[\"'`]"],
    [
      "(?:class|className|containerClassName|wrapperClassName|contentClassName|iconClassName)\\s*[:=]\\s*[\"'`]([^\"'`]*)[\"'`]"
    ],
    [
      "(?:class|className|containerClassName|wrapperClassName|contentClassName|iconClassName)\\s*[:=]\\s*{([^}]*)}"
    ]
  ],

  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.color": "#6633cc",
  "material-icon-theme.folders.associations": {
    "application": "api",
    "express": "routes",
    "protocols": "interface",
    "composers": "components",
    "strategy": "context",
    "factory": "generator",
    "adapters": "plugin",
    "slide": "svelte",
    "main": "core",
    "infra": "app",
    "presentation": "api",
    "entities": "class",
    "domain": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "providers": "include",
    "subscribers": "messages",
    "useCases": "controller",
    "kafka": "supabase",
    "mappers": "meta",
    "_shared": "shared",
    "eslint-config": "tools",
    "kube": "kubernetes",
    "nginx": "terraform",
    "internals": "app",
    "third_party": "other",
    "chglog": "changesets",
    "forms": "content"
  },
  "material-icon-theme.files.associations": {
    "vitest.config.ts": "vite",
    "ormconfig.json": "database",
    "*.proto": "3d",
    "*.webpack.js": "webpack",
    "*.build.tsx": "svgo",
    "*.bench.ts": "test-ts"
  },
  "material-icon-theme.languages.associations": {
    "dotenv": "tune"
  },
  "material-icon-theme.files.customClones": [
    {
      "name": "nest-service",
      "base": "nest",
      "color": "yellow-500",
      "activeForPacks": ["nest"],
      "fileExtensions": ["service.ts", "job.ts"]
    },
    {
      "name": "nest-service-test",
      "base": "test-ts",
      "color": "yellow-500",
      "activeForPacks": ["nest"],
      "fileExtensions": ["service.spec.ts", "job.spec.ts"]
    },
    {
      "name": "nest-adapter",
      "base": "nest",
      "color": "cyan-400",
      "activeForPacks": ["nest"],
      "fileExtensions": ["adapter.ts", "mapper.ts"]
    },
    {
      "name": "nest-gateway",
      "base": "nest",
      "color": "purple-400",
      "activeForPacks": ["nest"],
      "fileExtensions": ["repository.ts", "gateway.ts"]
    },
    {
      "name": "nest-decorator",
      "base": "nest",
      "color": "purple-500",
      "activeForPacks": ["nest"],
      "fileExtensions": ["decorator.ts"]
    },
    {
      "name": "nest-factory",
      "base": "nest",
      "color": "red-400",
      "activeForPacks": ["nest"],
      "fileExtensions": ["factory.ts", "strategy.ts"]
    },
    {
      "name": "nest-default",
      "base": "nest",
      "color": "gray-500",
      "activeForPacks": ["nest"],
      "fileExtensions": ["ts"]
    },
    {
      "name": "test-e2e",
      "base": "test-ts",
      "color": "cyan-300",
      "fileExtensions": ["e2e.spec.ts"]
    },
    {
      "name": "nest-controller",
      "base": "nest",
      "color": "blue-600",
      "fileExtensions": ["controller.ts", "controller.dto.ts"]
    }
  ],

  "cSpell.language": "en,pt",
  "cSpell.userWords": [
    "awsx",
    "clsx",
    "decrypter",
    "Decrypter",
    "deepseek",
    "encrypter",
    "gemini",
    "github",
    "langchain",
    "nginx",
    "openai",
    "orochaa",
    "picocolors",
    "postgres",
    "postgresql",
    "pulumi",
    "struct",
    "usecases"
  ],

  "security.workspace.trust.untrustedFiles": "newWindow",
  "console-ninja.featureSet": "Community",

  "jest.runMode": {
    "coverage": false,
    "deferred": false,
    "runAllTestsOnStartup": false,
    "showInlineError": true,
    "type": "on-demand"
  },

  "geminicodeassist.enableTelemetry": false,
  "geminicodeassist.languages": ["typescript", "typescriptreact", "go"],
  "geminicodeassist.inlineSuggestions.enableAuto": false,
  "geminicodeassist.project": "",

  "diffEditor.codeLens": true,

  "gitlens.keymap": "none",
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.views.scm.grouped.views": {
    "commits": true,
    "branches": true,
    "remotes": true,
    "stashes": true,
    "tags": true,
    "worktrees": true,
    "contributors": true,
    "repositories": false,
    "searchAndCompare": true,
    "launchpad": false
  },

  "liveServer.settings.donotShowInfoMsg": true,
  "geminicodeassist.updateChannel": "Default"
}
